{
    "manifest_version": 2,
    "name": "Google Search Vim Controls",
    "version": "2.0",
    "browser_action": {
        "default_icon": "128.png",
        "default_popup": "options.html"
    },
    "content_scripts": [
        {
            "matches": ["*://www.google.com/search*"],
            "js": ["script.js"]
        }
    ],
    "permissions": [
        "storage"
    ]
}

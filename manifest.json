{
    "manifest_version": 2,
    "name": "Google Search Vim Controls",
    "version": "0.1",
    "browser_action":
    {
        "default_icon": "128.png"
    },
    "content_scripts": [
        {
            "matches": ["*://www.google.com/search*"],
            "js": ["script.js"]
        }
    ]
}
